{
  "builds": [
    {
      "src": "app/flask_backend.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/app.html",
      "dest": "/app/app.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/app/flask_backend.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app/flask_backend.py"
    }
  ]
}